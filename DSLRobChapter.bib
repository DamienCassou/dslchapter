@inproceedings{Duijn:2005,
	Abstract = {Cognition is often interpreted in an anthropocentric manner, which induces an artificial dichotomy between genuine cognizers and reflexive/instinctive organisms. We propose that cognition is better interpreted from a non-anthropocentric perspective, allowing the lowest bounds of cognition to extend much further down the phylogenetic scale. In this article, we try to explicate the minimal requirements for cognition by teasing it apart from more basic forms of ontogenetic adaptation. We argue that minimal cognition can already be found in organisms without nervous systems, and develop the claim that minimal cognition requires a sensorimotor mechanism that subsumes a basic metabolic network; grounding the increasingly popular idea that cognition revolves around perception-action couplings. We bring this conception of minimal cognition to bear on a case study of the Escherichia coli bacterium, and use it to refine the general idea. The relevance of this interpretation of minimal cognition for the study of cognition and for the understanding of cognition in general is discussed.
},
	Author = {van Duijn, Marc and Keijzer, Fred and Franken, Daan},
	Booktitle = {Active Agents and their environments as dynamical systems workshop - ECAL 2005},
